{
    "env": {
        "browser": true,
        "node": true,
        "es6": true,
        "jest": true
    },
    "parserOptions": {
        "ecmaVersion": 2022,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "rules": {
        "react/react-in-jsx-scope": "off",
        // "react/prop-types": "off",
        "prefer-destructuring": [
            "error",
            {
                "array": true,
                "object": true
            }
        ]
    }
}